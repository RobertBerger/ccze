# Common definitions
if test -z "$srcdir"; then
	srcdir=`echo "$0" | sed 's,[^/]*$,,'`
	test "$srcdir" = "$0" && srcdir=.
	test -z "$srcdir" && srcdir=.
	test "${VERBOSE+set}" != set && VERBOSE=1
fi
. $srcdir/defs

TEST_OK_LIST="$testname.ok"

cp ${srcdir}/$testname.ok $testname.ok
cp ${srcdir}/$testname.in $testname.in

PROG_CMDLINE="-F /dev/null -p syslog -o nolookups -d"

. ${srcdir}/do-test
