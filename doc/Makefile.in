## doc/Makefile.in

## -- User-modifiable variables -- ##
srcdir		= @srcdir@
top_srcdir	= @top_srcdir@
subdir		= doc

## == The code below shouldn't need to be touched at all == ##

include ../Rules.mk

## -- Private variables -- ##
MAN		= ccze.1

## -- Generic rules -- ##
all: ;

install: all
	${INSTALL} -d ${DESTDIR}${man1dir}
	${INSTALL_DATA} ${MAN} ${DESTDIR}${man1dir}/

uninstall:
	rm -f ${DESTDIR}${man1dir}/${MAN}
